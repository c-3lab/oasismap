{
  "extends": ["next/core-web-vitals", "prettier", "eslint:recommended"],
  "globals": {
    "React": true,
    "GeolocationPosition": true
  },
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "caughtErrors": "all",
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ]
  },
  "plugins": ["@typescript-eslint"]
}
